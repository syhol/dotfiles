{
  "editor.formatOnSave": true,
  "files.autoSave": "onFocusChange",
  "editor.fontFamily": "JetBrainsMono NFM, MesloLGSDZ Nerd Font Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "terminal.integrated.fontFamily": "JetBrainsMono NFM",
  "terminal.integrated.fontSize": 14,
  "excalidraw.theme": "auto",
  "deno.cacheOnSave": true,
  "redhat.telemetry.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "[json][typescript][jsonc][javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.renderFinalNewline": "off",
  "files.insertFinalNewline": true,
  // "editor.rulers": [80, 120],
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "args": ["-l"],
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "args": ["-l"]
    },
    "fish": {
      "path": "fish",
      "args": ["-l"]
    }
  },
  "mise.binPath": "/opt/homebrew/bin/mise",
  "terminal.integrated.shellIntegration.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.colorTheme": "Catppuccin Macchiato",
  "gitlens.plusFeatures.enabled": false
}
